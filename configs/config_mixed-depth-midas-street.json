{
    "expname": "maio/mixed-depth-midas-street-big",
    "crop_center": true,
    "dataset": [
        "synthia",
        "synscapes"
        
    ],
    "augmentation": true,
    "encoder_name": "large",
    "num_workers": 4,
    "target_size": [
        256,
        256
    ],
    "depth_clip": null,
    "mask_sky": false,
    "dataset_root": [
        "/hadatasets/SYNTHIA-AL/",
        "/hadatasets/synscapes"
    ],
    "batch_size": 4,
    "epochs": 150,
    "split_json": [
        "configs/synthia_splits.json",
        "configs/synscapes_split.json"
    ],
    "features": [
        [
            "image_l"
        ],
        [
            "depth_l"
        ]
    ],
    "tasks": [
        {
            "type": "dense_regression",
            "name": "depth",
            "train_on_disparity": false,
            "loss": "midas",
            "metrics": [
                "rmse",
                "logrmse",
                "absrel",
                "squaredrel",
                "a1",
                "a2",
                "a3"
            ],
            "channels": 1,
            "features": [
                "depth_l"
            ],
            "mask_feature": null,
            "decoder": {
                "type": "unet",
                "args": {}
            }
        }
    ]
}